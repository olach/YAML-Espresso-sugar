<settings>
	<language id="com.github.olach.Espresso.Sugar.YAML">
		<name>YAML</name>
		<root-zone>language-root.yaml</root-zone>
		
		<detectors>
			<extension>yml</extension>
			<extension>yaml</extension>
		</detectors>
		
		<default-extension>yml</default-extension>
		<default-mime-type>application/x-yaml</default-mime-type>
	</language>
</settings>
